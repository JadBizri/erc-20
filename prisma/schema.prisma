generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Transfer {
  transactionHash String  @id @unique
  blockNumber     Int?
  fromAddress     String?
  toAddress       String?
  amount          String?
  tokenAddress    String?
}

model Sync_Data {
  key    String @unique
  number Int
}
